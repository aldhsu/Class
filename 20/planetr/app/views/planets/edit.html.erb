<form action="<%= planet_path @planet %>" method="post">
  <p>Name<input type="text" name="planet[name]" autofocus value="<%= @planet.name %>"></p>
  <p>Images<input type="url" name="planet[image]" value="<%= @planet.image %>"></p>
  <p>Diameter<input type="text" name="planet[diameter]" value="<%= @planet.diameter %>"></p>
  <p>Mass<input type="text" name='planet[mass]' value="<%= @planet.mass %>"></p>
  <p>Moons<input type="text" name ='planet[moons]' value="<%= @planet.moons %>"></p>
  <button>Submit</button>
</form>